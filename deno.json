{
  "name": "@tichopad/alacritty-theme-switch",
  "version": "3.0.0-beta.2",
  "license": "MIT",
  "tasks": {
    "check": "deno fmt --check && deno lint && deno check",
    "dev": "deno run --allow-write --allow-env --allow-read --watch ./src/main.ts",
    "run": "deno run --allow-write --allow-env --allow-read ./src/main.ts",
    "test": "deno test --allow-write --allow-env --allow-read ./tests/unit",
    "build-npm": "deno run --allow-write --allow-env --allow-read --allow-run scripts/build-npm.ts"
  },
  "exports": "./src/main.ts",
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@0.42.3",
    "@inquirer/search": "npm:@inquirer/search@3.1.1",
    "@std/assert": "jsr:@std/assert@1.0.13",
    "@std/cli": "jsr:@std/cli@1.0.21",
    "@std/fs": "jsr:@std/fs@1.0.19",
    "@std/path": "jsr:@std/path@1.1.2",
    "@std/toml": "jsr:@std/toml@1.0.8"
  }
}
